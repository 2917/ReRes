{
    "name": "ReRes",
    "version": "1.0",
    "description": "change the response.",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "ReRes",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
} 